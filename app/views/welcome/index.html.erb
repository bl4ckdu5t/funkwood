<% flash.each do |name, msg| %>
  <%= content_tag :div, msg, :class => 'notifier' %>
<% end %>
<div class="top">
	<h1 class="title">muster<span>d8</span></h1>
	<%= link_to('<button data-button="curvy transparent">Member sign in</button>'.html_safe, new_user_session_path)  %> 
</div>
<header class="header-index">
	<div class="caption">
		<span>Find your date in a click</span>
		<h2>A virtual love garden</h2>
		<a href="<%= root_url %>#steps"><button data-button="curvy transparency">Learn how it works</button></a>
	</div>
	<form action="" id="js-register">
		<input type="text" name="name" id="js-name" placeholder="My name is?" required>
		<select name="age" id="js-age" required>
			<option value="" disabled selected>My age is?</option>
			<% (18..70).each do |age| %>
			<option value="<%= age %>"><%= age %> years</option>
			<% end %>
		</select>
		<select name="gender" id="js-gender" required>
			<option value="" disabled selected>I am a?</option>
			<option value="male">Man</option>
			<option value="female">Woman</option>
		</select>
		<input type="submit" data-button="material" value="Find your date">
	</form>
</header>
